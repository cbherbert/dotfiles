\begin{equation}
	\frac{\partial P(x,t)}{\partial t} = - \frac{\partial}{\partial x} \lbrack a(x,t)P(x,t)\rbrack + \epsilon \frac{\partial^2}{\partial x^2} P(x,t).
\end{equation}
